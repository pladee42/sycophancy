{
  "apiEndpoint": "https://openrouter.ai/api/v1/chat/completions",
  "defaultModel": "openai/gpt-4.1",
  "maxTokens": 4096,
  "temperature": 0.7,
  "debug": true,
  "availableModels": [
    {
      "id": "openai/gpt-4.1",
      "name": "GPT-4.1",
      "provider": "OpenAI"
    },
    {
      "id": "openai/gpt-3.5-turbo",
      "name": "GPT-3.5 Turbo",
      "provider": "OpenAI"
    },
    {
      "id": "meta-llama/llama-4-maverick",
      "name": "Llama 4 Meverick",
      "provider": "Meta"
    },
    {
      "id": "anthropic/claude-sonnet-4",
      "name": "Claude Sonnet 4",
      "provider": "Anthropic"
    },
    {
      "id": "google/gemini-2.5-pro",
      "name": "Gemini 2.5 Pro",
      "provider": "Google"
    },
    {
      "id": "x-ai/grok-4",
      "name": "Grok 4",
      "provider": "xAI"
    }
  ],
  "colorPalette": {
    "primary": "#284B63",
    "primaryVariant": "#3700B3",
    "secondary": "#03DAC6",
    "background": "#121212",
    "surface": "#121212",
    "error": "#CF6679",
    "onPrimary": "#000000",
    "onSecondary": "#000000",
    "onBackground": "#FFFFFF",
    "onSurface": "#FFFFFF",
    "onError": "#000000"
  },
  "providerColors": {
    "OpenAI": "#10A37F",
    "Anthropic": "#D4A373",
    "Google": "#4285F4",
    "Meta": "#0866FF",
    "xAI": "#FFFFFF"
  },
  "antiSycophancyEngine": {
    "baseDisagreementMin": 15,
    "baseDisagreementMax": 30,
    "contextualAdjustmentStrength": 0.3,
    "parameterInteractionStrength": 0.2,
    "adaptiveLearningRate": 0.1,
    "topicModifiers": {
      "personal": 0.7,
      "factual": 1.2,
      "opinion": 1.0,
      "political": 0.8,
      "philosophical": 1.3,
      "technical": 1.1,
      "creative": 0.9,
      "emotional": 0.6
    },
    "challengeTypes": {
      "devils_advocate": {
        "enabled": true,
        "intensity": 0.8,
        "description": "Present strongest counterarguments"
      },
      "socratic": {
        "enabled": true,
        "intensity": 0.6,
        "description": "Use questioning to examine assumptions"
      },
      "perspective_shift": {
        "enabled": true,
        "intensity": 0.7,
        "description": "Introduce alternative viewpoints"
      },
      "evidence_challenge": {
        "enabled": true,
        "intensity": 0.9,
        "description": "Request evidence or challenge logic"
      },
      "nuance_injection": {
        "enabled": true,
        "intensity": 0.5,
        "description": "Add complexity to oversimplified statements"
      }
    },
    "responseQuality": {
      "minChallengeGap": 3,
      "maxAgreementStreak": 4,
      "intellectualHumilityThreshold": 0.3,
      "factCheckingEnabled": true,
      "contradictionDetection": true
    },
    "adaptiveSettings": {
      "learningEnabled": false,
      "conversationMemory": 10,
      "userPatternTracking": true,
      "effectivenessMetrics": true
    }
  }
}